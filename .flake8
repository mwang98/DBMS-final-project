[flake8]
max-line-length = 100
ignore =
    E125,E121,E266,E402,E741
    # print is allowed
    T001, T003
exclude =
    .git
    # __pycache__
    __pycache__
    # virtual environment
    .venv/
    venv/
    env/
    # old api
    # __init__s usually have pre-imported modules
    python-dependencies/

max_complexity = 11
statistics = true
