dbrp "T_24t2WRbHGi9rXFjSf4vfNW"."autogen"
var data = stream
    |from()
        .measurement('M')
    |window()
        .period(10s)
        .every(10s)

data
    @tTest()
        .field('f')
        .size(500)
        .detector_type('ttest')
        .detector_params('{"alpha": 0.01}')
    |alert()
        .id('f')
        .crit(lambda: "is_anomaly")
        .log('/tmp/kapacitor_udf/T_24t2WRbHGi9rXFjSf4vfNW_failure.log')
        |influxDBOut()
            .database('D')
            .retentionPolicy('autogen')
            .measurement('T_24t2WRbHGi9rXFjSf4vfNW_alert')
    
data
    |influxDBOut()
        .create()
        .database('D')
        .retentionPolicy('autogen')
        .measurement('T_24t2WRbHGi9rXFjSf4vfNW')

